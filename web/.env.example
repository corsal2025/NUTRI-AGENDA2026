# Web Next.js (web/)
# Copia a `web/.env.local`

# Supabase (public)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Base URL del sitio (para callbacks)
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# MercadoPago (server-side)
MERCADOPAGO_ACCESS_TOKEN=
MERCADOPAGO_WEBHOOK_SECRET=

# Google OAuth (server-side) - Calendar
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Resend (server-side) - Emails
RESEND_API_KEY=
